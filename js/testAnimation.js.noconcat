
testA = {};

testA.cnt = 0;

testA.ani = null;

testA.testAnimation = function()
{

	testA.ani = cy.animation({

		complete: function(){

			testA.cnt++;
			console.log("" + testA.cnt + ' bagel')

			if(testA.cnt < 20)
				testA.testAnimation();

		},

		duration: 1000

	});

	testA.ani.play();

}

